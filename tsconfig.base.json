{
  "compilerOptions": {
    "composite": true,
    "declarationMap": true,
    "emitDeclarationOnly": false,
    "importHelpers": true,
    "isolatedModules": true,
    "lib": [
      "es2022"
    ],
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es2022",
    "customConditions": [
      "development"
    ],
    "baseUrl": ".",
    "paths": {
      "@twitch-monorepo-nx/protos": [
        "libs/protos/src/index.ts"
      ],
      "@twitch-monorepo-nx/protos/generated/*": [
        "libs/protos/src/generated/*"
      ]
    }
  },
  "references": [
    {
      "path": "libs/protos/tsconfig.lib.json"
    }
  ]
}
